type Query {
    groupById(id: Int!): Group
    getGroups: [Group!]
}

type Group {
    id: Int
    slug: String
    displayName: String
    hashedId: String
    description: String
    iconUrl: String
    bannerUrl: String
    nsfw: Boolean
    isPrivate: Boolean
    createdAt: String
    updatedAt: String
}